{
  "name": "p5sketch-template",
  "version": "1.0.0",
  "description": "",
  "main": "index.html",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "mkdir -p ./dist && yarn clean && yarn copy && yarn build:dist",
    "start": "mkdir -p ./dist && yarn clean && yarn copy && yarn start:dev",
    "copy": "cp ./public/* ./dist",
    "clean": "rm -rf ./dist/*",
    "build:dist": "esbuild ./src/index.js --bundle --minify --sourcemap --outdir=./dist --target=es2020",
    "start:dev": "esbuild ./src/index.js --bundle --sourcemap --outdir=./dist --watch --servedir=./dist --target=es2020",
    "prepare": "npx crlf --set=LF node_modules/.bin/*"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/conceptblend/p5sketch-template.git"
  },
  "keywords": [
    "p5",
    "p5js",
    "processing",
    "template"
  ],
  "author": "Andrew J Wright",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/conceptblend/p5sketch-template/issues"
  },
  "homepage": "https://github.com/conceptblend/p5sketch-template#readme",
  "dependencies": {
    "@types/p5": "^1.3.2",
    "esbuild": "0.17.8",
    "p5": "^1.5.0",
    "p5.js-svg": "^1.3.3"
  }
}
